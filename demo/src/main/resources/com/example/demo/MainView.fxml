<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.MainController">
  <ListView fx:id="productListView" />
  <Button text="Add to Cart" fx:id="addButton" onAction="#addToCart"/>
  <Button text="Place Order" fx:id="placeOrderButton" onAction="#placeOrder"/>
</VBox>
